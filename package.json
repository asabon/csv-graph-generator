{
    "name": "csv-graph-generator",
    "version": "0.0.5",
    "private": true,
    "description": "GitHub Action to generate graph images from CSV files",
    "main": "lib/main.js",
    "scripts": {
        "build": "tsc",
        "format": "prettier --write '**/*.ts'",
        "format-check": "prettier --check '**/*.ts'",
        "lint": "eslint src/**/*.ts",
        "package": "ncc build src/main.ts -o dist --source-map --license licenses.txt",
        "test": "jest",
        "all": "npm run build && npm run format && npm run package"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/asabon/csv-graph-generator.git"
    },
    "keywords": [
        "actions",
        "node",
        "csv",
        "graph",
        "chart"
    ],
    "author": "asabon",
    "license": "MIT",
    "dependencies": {
        "@actions/core": "^1.10.0",
        "chart.js": "^3.9.1",
        "chartjs-node-canvas": "^4.1.6",
        "csv-parse": "^5.5.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/jest": "^29.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@vercel/ncc": "^0.38.0",
        "eslint": "^8.0.0",
        "eslint-plugin-jest": "^27.0.0",
        "jest": "^29.0.0",
        "js-yaml": "^4.1.0",
        "prettier": "^3.0.0",
        "ts-jest": "^29.0.0",
        "typescript": "^5.0.0"
    }
}
